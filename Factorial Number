#include <stdio.h>

int main() {
    int n, i;
    long long fact = 1;

    // Input
    printf("Enter a number: ");
    scanf("%d", &n);

    // Factorial calculation
    for (i = 1; i <= n; i++) {
        fact *= i;
    }

    // Output
    printf("Factorial of %d = %lld\n", n, fact);

    return 0;
}
