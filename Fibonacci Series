#include <stdio.h>

int main() {
    int n, i;
    long long a = 0, b = 1, next;

    // Input
    printf("Enter how many terms you want in Fibonacci series: ");
    scanf("%d", &n);

    printf("Fibonacci Series: ");

    // Print first two terms
    for (i = 1; i <= n; i++) {
        printf("%lld ", a);
        next = a + b;
        a = b;
        b = next;
    }

    return 0;
}
