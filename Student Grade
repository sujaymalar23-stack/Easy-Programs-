#include <stdio.h>

int main() {
    int marks;

    // Input
    printf("Enter the student's marks (0â€“100): ");
    scanf("%d", &marks);

    // If-else ladder for grading
    if (marks >= 90) {
        printf("Grade: A\n");
    }
    else if (marks >= 80) {
        printf("Grade: B\n");
    }
    else if (marks >= 70) {
        printf("Grade: C\n");
    }
    else if (marks >= 60) {
        printf("Grade: D\n");
    }
    else if (marks >= 50) {
        printf("Grade: E\n");
    }
    else if (marks >= 0) {
        printf("Grade: F (Fail)\n");
    }
    else {
        printf("Invalid marks!\n");
    }

    return 0;
}
